{
  "name": "chatgpt-mattermost-bot",
  "version": "1.3.4",
  "description": "ChatGPT bot on mattermost",
  "private": true,
  "keywords": [
    "chatgpt",
    "bot",
    "ai",
    "openai",
    "mattermost"
  ],
  "main": "src/botservice.js",
  "type": "module",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/takuya-o/chatgpt-mattermost-bot/issues"
  },
  "homepage": "https://github.com/takuya-o/chatgpt-mattermost-bot#readme",
  "files": [
    "out/**/*"
  ],
  "scripts": {
    "product": "node ./out/botservice.mjs",
    "start": "node ./out/botservice.mjs",
    "dev": "ts-node ./src/botservice.ts",
    "upgrade": "npx --yes npm-check-updates -u && npm update && npm dedup",
    "clean": "rm -rf .npm/ node_modules/ out/*; npm ci",
    "build": "rimraf out && node esbuild.config.js",
    "lint": "eslint src --ext ts && prettier -c src && tsc --noEmit",
    "format": "prettier --write src",
    "test": "echo 'There is no test.'"
  },
  "dependencies": {
    "@mattermost/client": "^7.10.0",
    "@mattermost/types": "^7.10.0",
    "debug-level": "3.0.0",
    "form-data": "^4.0.0",
    "openai": "^3.3.0",
    "tiktoken-node": "^0.0.6",
    "ws": "^8.13.0"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^20.4.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "esbuild": "^0.18.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
